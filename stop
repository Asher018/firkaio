[32m+[m[32mimport imp[m
[32m+[m[32mfrom flask import Flask, render_template[m
[32m+[m[32mfrom flask_socketio import SocketIO[m
[32m+[m[32mfrom engineio.payload import Payload[m
[32m+[m[32mimport random[m
[32m+[m[32mfrom eventlet import wsgi[m
[32m+[m[32mimport eventlet[m
[32m+[m
[32m+[m
[32m+[m[32mapp = Flask(__name__)[m
[32m+[m[32m# Payload.max_decode_packets = 999999[m
[32m+[m[32msocketio = SocketIO(app)[m
[32m+[m
[32m+[m[32m@app.route("/")[m
[32m+[m[32mdef index():[m
[32m+[m[32m    return render_template("index.html")[m
[32m+[m
[32m+[m
[32m+[m[32m@app.route("/nezo")[m
[32m+[m[32mdef nezo():[m
[32m+[m[32m    return render_template("nezo.html")[m
[32m+[m
[32m+[m
[32m+[m[32m@app.route("/rajzolo")[m
[32m+[m[32mdef rajzolo():[m
[32m+[m[32m    return render_template("rajzolo.html")[m
[32m+[m
[32m+[m[32mdef give_random_word():[m
[32m+[m[32m    dictionary = [[m
[32m+[m[32m        "dog",[m
[32m+[m[32m        "cat",[m
[32m+[m[32m        "house",[m
[32m+[m[32m        "head",[m
[32m+[m[32m        "number",[m
[32m+[m[32m        "tree",[m
[32m+[m[32m        "sky",[m
[32m+[m[32m        "sun",[m
[32m+[m[32m        "boat",[m
[32m+[m[32m        "elbow",[m
[32m+[m[32m        "chimney",[m
[32m+[m[32m        "computer",[m
[32m+[m[32m        "flag",[m
[32m+[m[32m        "bird",[m
[32m+[m[32m        "fish",[m
[32m+[m[32m        "money",[m
[32m+[m[32m        "bow",[m
[32m+[m[32m        "axe",[m
[32m+[m[32m        "scissors",[m
[32m+[m[32m        "glasses",[m
[32m+[m[32m    ][m
[32m+[m[32m    return random.choice(dictionary)[m
[32m+[m
[32m+[m
[32m+[m[32m@socketio.on("give_random_word")[m
[32m+[m[32mdef random_word(rnd_word=""):[m
[32m+[m[32m    global global_word[m
[32m+[m[32m    global_word=give_random_word()[m
[32m+[m[32m    #print(f"fgv bel√ºl: {global_word}")[m
[32m+[m[32m    socketio.emit("give_random_word", global_word)[m
[32m+[m
[32m+[m[32m@socketio.on("message")[m
[32m+[m[32mdef print_message(message):[m
[32m+[m[32m    print(message)[m
[32m+[m[32m    msg_index = int(message.index(':'))[m
[32m+[m[32m    win_name = message[0:msg_index][m
[32m+[m[32m    winner_name(win_name)[m
[32m+[m[32m    #print(f"Global_word:{global_word}, Msg_word:{msg_word}.")[m
[32m+[m[32m    if str(global_word) in str(message):[m
[32m+[m[32m        print("equal")[m
[32m+[m[32m        game_end()[m
[32m+[m[32m    socketio.emit("message", message)[m
[32m+[m
[32m+[m
[32m+[m[32m@socketio.on("drawing")[m
[32m+[m[32mdef drawing(message):[m
[32m+[m[32m    socketio.emit("drawing", message)[m
[32m+[m
[32m+[m[32m@socketio.on("game_end")[m
[32m+[m[32mdef game_end():[m
[32m+[m[32m    print("Game over")[m
[32m+[m[32m    socketio.emit("game_end")[m
[32m+[m
[32m+[m[32m@socketio.on("winner_name")[m
[32m+[m[32mdef winner_name(name):[m
[32m+[m[32m    socketio.emit("winner_name", name)[m
[32m+[m
[32m+[m[32m@socketio.on("new_game")[m
[32m+[m[32mdef new_game():[m
[32m+[m[32m    socketio.emit("new_game")[m
[32m+[m
[32m+[m[32m@socketio.on("viewer_joinned")[m
[32m+[m[32mdef viewer_joinned():[m
[32m+[m[32m    print("viewer_joinned")[m
[32m+[m[32m    #random_word()[m
[32m+[m[32m    socketio.emit("viewer_joinned")[m
[32m+[m
[32m+[m[32mif __name__ == '__main__':[m
[32m+[m[32m  app.debug=True[m
[32m+[m[32m  wsgi.server(eventlet.listen(('',8080)), app)[m
[32m+[m
